{
    "name": "aSmithSummer/roadblock_country",
    "description": "Extends roadblock to provide country support",
    "type": "silverstripe-vendormodule",
    "keywords": [
        "silverstripe",
        "CMS"
    ],
    "license": "BSD-3-Clause",
    "require": {
        "asmithsummer/roadblock": "dev-develop"
    },
    "require-dev": {
        "phpunit/phpunit": "^9.5",
        "squizlabs/php_codesniffer": "^3.7"
    },
    "autoload": {
        "psr-4": {
            "aSmithSummer\\RoadblockCountry\\": "src/",
            "aSmithSummer\\RoadblockCountry\\Tests\\": "tests/php/"
        }
    },
    "scripts": {
        "test": [
            "@test-phpunit"
        ],
        "test-phpunit": "phpunit -d memory_limit=512M -d flush=1",
        "phpcs": "phpcs src --standard=phpcs.xml.dist --extensions=php --encoding=utf-8",
        "phpcbf": "phpcbf src --standard=phpcs.xml.dist --extensions=php --encoding=utf-8",
        "phpcs-fix": "@phpcbf"
    },
    "minimum-stability": "dev",
    "prefer-stable": true
}
